{
  "name": "jkohlin/wordpress-basics",
  "version": "0.1.4",
  "libraries": {
    "xv": "^1.1.25"
  },
  "title": "Compositor demo",
  "branch": "",
  "style": {
    "name": "Humanist",
    "componentSet": {
      "nav": "nav/BasicNav",
      "header": "header/LightBannerHeader",
      "article": "article/ReaderArticle",
      "footer": "footer/BasicFooter"
    },
    "fontFamily": "Athelas, Georgia, serif",
    "heading": {
      "fontWeight": 400,
      "textTransform": "uppercase",
      "letterSpacing": "0.1em"
    },
    "colors": {
      "text": "#121212",
      "background": "#fff",
      "primary": "#ee5544",
      "secondary": "#005499",
      "highlight": "#00bd78",
      "border": "#ddd",
      "muted": "#f3f3f3"
    },
    "layout": {
      "maxWidth": 768
    }
  },
  "content": [
    {
      "component": "nav",
      "links": [
        {
          "href": "https://github.com/jkohlin/wordpress-basics",
          "text": "GitHub"
        }
      ]
    },
    {
      "component": "header",
      "heading": "wordpress-basics",
      "subhead": "Learn Wordpress and how to build a new theme from scratch",
      "children": [
        {
          "component": "ui/TweetButton",
          "text": "wordpress-basics: Learn Wordpress and how to build a new theme from scratch",
          "url": null
        },
        {
          "component": "ui/GithubButton",
          "user": "jkohlin",
          "repo": "wordpress-basics"
        }
      ]
    },
    {
      "component": "article",
      "metadata": {
        "source": "github.readme"
      },
      "html": "\n\n"
    },
    {
      "component": "footer",
      "links": [
        {
          "href": "https://github.com/jkohlin/wordpress-basics",
          "text": "GitHub"
        },
        {
          "href": "https://github.com/jkohlin",
          "text": "jkohlin"
        }
      ]
    }
  ]
}